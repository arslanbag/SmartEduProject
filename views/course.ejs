<!DOCTYPE html>
<html lang="en">
	<!-- Start head -->
	<%- include('partials/_head.ejs') -%>
<body class="host_version"> 

      <!-- Start loader -->
	  <%- include('partials/_loader.ejs') -%>
	  <!-- Start header -->
	  <%- include('partials/_header.ejs') -%>
	
	<div class="all-title-box">
		<div class="container text-center">
			<h1><%= course.name %><span class="m_1">Lorem Ipsum dolroin gravida nibh vel velit.</span></h1>
		</div>
	</div>
	
    <div id="overviews" class="section wb">
        <div class="container">
            <div class="row"> 
                <div class="col-lg-9 blog-post-single">
                    <div class="blog-item">
						<div class="image-blog">
							<img src="/images/blog_single.jpg" alt="" class="img-fluid">
						</div>
						<div class="post-content">
							<div class="post-date">
								<!-- enroll or relase -->
								<%- include('partials/_enrollOrRelase') %>
							</div>
							<div class="meta-info-blog">
								<span><i class="fa fa-calendar"></i> <a href="#">May 11, 2015</a> </span>
							</div>
							<div class="blog-title">
								<h2><a href="#" title=""><%= course.name %></a></h2>
							</div>
							<div class="blog-desc">

								<blockquote class="default">
										<%= course.description %>
								</blockquote>

							</div>							
						</div>
					</div>
					
					<!-- blog author -->
					<%- include('partials/_blogAuthor.ejs') -%>
					
                </div><!-- end col -->
				<div class="col-lg-3 col-12 right-single">
					<div class="widget-search">
						<!-- course search -->
						<%- include('partials/_searchCourse.ejs') %>	
					</div>
					<div class="widget-categories">
						<div class="widget-categories">
							<!-- categories -->
							<%- include('partials/_categories.ejs') %>
						</div>
					</div>
				</div>
            </div><!-- end row -->
        </div><!-- end container -->
    </div><!-- end section -->

<!-- Start footer -->
<%- include('partials/_footer.ejs') -%>

</body>
</html>